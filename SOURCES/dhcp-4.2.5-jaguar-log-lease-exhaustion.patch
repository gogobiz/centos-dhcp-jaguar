+		if (status) {
+			log_error("log_lease_exhaustion: %s exit status %d", argv[0], status);
+		}
+	} else if (pid == 0) {
+		execvp(argv[0], argv);
+		log_error("log_lease_exhaustion: Unable to execute %s: %m", argv[0]);
+		_exit(127);
+	} else {
+		log_error("log_lease_exhaustion: fork() failed");
+	}
